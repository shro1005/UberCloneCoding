{
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "linterOptions": {
        "exclude": ["config/**/*.js", "node_modules/**/*."]
    },
    "rules": {
        "no-console": false,
        "member-access": false,
        "object-literal-sort-key": false,
        "ordered-imports": true,
        "interface-name": false,
        "strict-null-checks": false
    },
    "ruleDirectory": []
}